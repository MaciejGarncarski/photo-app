{
	"name": "backend",
	"type": "module",
	"private": true,
	"version": "0.0.0",
	"scripts": {
		"build": "pnpm prisma generate && pnpm tsc",
		"build:dev": "pnpm prisma generate && pnpm tsc",
		"start": "node --env-file=.env ./dist/src",
		"restart-deploy": "pm2 startOrRestart ~/ecosystem.config.js --name \"Photo app - backend\"",
		"check-types": "tsc --noEmit",
		"test": "vitest",
		"coverage": "vitest run --coverage",
		"dev": "pnpm tsx watch --clear-screen=false --env-file=.env ./src/index.ts"
	},
	"dependencies": {
		"@fastify/cookie": "^9.3.1",
		"@fastify/cors": "^9.0.1",
		"@fastify/multipart": "^8.3.0",
		"@fastify/oauth2": "^7.8.1",
		"@fastify/sensible": "^5.6.0",
		"@fastify/session": "^10.9.0",
		"@fastify/swagger": "^8.14.0",
		"@fastify/swagger-ui": "^4.0.0",
		"@fastify/type-provider-typebox": "^4.0.0",
		"@prisma/client": "^5.15.0",
		"@quixo3/prisma-session-store": "^3.1.13",
		"@sinclair/typebox": "^0.31.17",
		"argon2": "^0.40.3",
		"fastify": "^4.28.0",
		"fastify-plugin": "^4.5.1",
		"fastify-socket.io": "^5.0.0",
		"imagekit": "^5.0.1",
		"ms": "^2.1.3",
		"nanoid": "^5.0.7",
		"pg": "^8.11.5",
		"postgres": "^3.4.4",
		"prisma": "^5.15.0",
		"socket.io": "^4.7.5"
	},
	"devDependencies": {
		"@faker-js/faker": "^8.4.1",
		"@swc/core": "1.5.29",
		"@types/ms": "^0.7.34",
		"@vitest/coverage-v8": "^1.6.0",
		"nodemon": "^3.1.3",
		"tsx": "^4.15.5",
		"typescript": "^5.4.5",
		"vite-tsconfig-paths": "^4.3.2",
		"vitest": "1.6.0",
		"vitest-mock-extended": "^1.3.1"
	}
}
