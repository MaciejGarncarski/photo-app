{
	"name": "frontend",
	"description": "Fullstack Next,js social app. In photo-app you can sign up via google and upload your favourite photos.",
	"version": "0.0.0",
	"scripts": {
		"dev": "next dev --turbo",
		"build": "NEXT_TELEMETRY_DISABLED=1 next build --turbo",
		"build:analyze": "ANALYZE=true next build",
		"start": "next start",
		"check-types": "tsc --skipLibCheck --noEmit",
		"format:check": "prettier -c .",
		"vercel-build": "next build",
		"generate-types": "openapi-typescript http://localhost:3001/documentation/json --output types.ts",
		"test": "vitest",
		"coverage": "vitest run --coverage"
	},
	"dependencies": {
		"@hookform/resolvers": "^4.1.3",
		"@phosphor-icons/react": "^2.1.7",
		"@qdrant/openapi-typescript-fetch": "^1.2.6",
		"@radix-ui/react-dropdown-menu": "^2.1.6",
		"@radix-ui/react-tooltip": "^1.1.8",
		"@tanstack/react-query": "^5.71.1",
		"clsx": "^2.1.1",
		"framer-motion": "^12.6.2",
		"jotai": "^2.12.2",
		"lodash.debounce": "^4.0.8",
		"nanoid": "^5.1.5",
		"next": "15.3.0-canary.26",
		"next-themes": "^0.4.6",
		"react": "19.0.0",
		"react-dom": "19.0.0",
		"react-easy-crop": "^5.4.1",
		"react-focus-lock": "^2.13.6",
		"react-hook-form": "^7.55.0",
		"react-intersection-observer": "^9.16.0",
		"react-remove-scroll": "^2.6.3",
		"react-responsive": "^10.0.1",
		"socket.io-client": "^4.8.1",
		"sonner": "^2.0.2",
		"zod": "^3.24.2"
	},
	"devDependencies": {
		"@csstools/postcss-global-data": "^3.0.0",
		"@next/bundle-analyzer": "15.2.1",
		"@playwright/test": "^1.51.1",
		"@tanstack/react-query-devtools": "^5.71.1",
		"@types/lodash.debounce": "^4.0.9",
		"@types/node": "^22.13.14",
		"@types/react": "^19.0.12",
		"@types/react-dom": "^19.0.4",
		"autoprefixer": "^10.4.21",
		"openapi-typescript": "^7.6.1",
		"postcss": "^8.5.3",
