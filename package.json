{
  "name": "photo-app",
  "description": "Fullstack Next,js social app. In photo-app you can sign up via google and upload your favourite photos.",
  "version": "0.2.0",
  "private": true,
  "lint-staged": {
    "**/*.{js,ts,jsx,tsx}": "pnpm lint",
    "**/*.{js,jsx,ts,tsx,md,mdx}": "pnpm format"
  },
  "proxy": "http://localhost:3001",
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "prepare": "pnpm husky install",
    "typecheck": "tsc --skipLibCheck --noEmit",
    "format:check": "prettier -c .",
    "vercel-build": "next build",
    "postinstall": "husky install",
    "generate-types": "openapi-typescript http://localhost:3001/documentation/json --output types.ts",
    "lint": "eslint --fix --max-warnings=0 --no-warn-ignored",
    "format": "prettier --write  \"**/*.{js,jsx,ts,tsx,md,mdx}\"",
    "lint-format": "pnpm format && pnpm lint",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.1",
    "@next/bundle-analyzer": "^14.2.3",
    "@phosphor-icons/react": "^2.0.13",
    "@qdrant/openapi-typescript-fetch": "^1.2.6",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@tanstack/react-query": "^5.36.1",
    "clsx": "^2.1.1",
    "framer-motion": "^11.2.2",
    "imagekit": "^5.0.1",
    "jotai": "^2.8.0",
    "lodash.debounce": "^4.0.8",
    "next": "^14.2.3",
    "next-themes": "^0.3.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-easy-crop": "^5.0.7",
    "react-focus-lock": "^2.12.1",
    "react-hook-form": "^7.51.4",
    "react-intersection-observer": "^9.10.2",
    "react-remove-scroll": "^2.5.10",
    "react-responsive": "^10.0.0",
    "sass": "^1.77.1",
    "sharp": "^0.33.3",
    "socket.io-client": "^4.7.5",
    "sonner": "^1.4.41",
    "typescript": "5.4.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@faker-js/faker": "^8.2.0",
    "@tanstack/react-query-devtools": "^5.36.1",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/lodash.debounce": "^4.0.9",
    "@types/node": "^20.7.0",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.1.0",
    "@vitest/coverage-v8": "^0.34.6",
    "bufferutil": "^4.0.7",
    "cypress": "^13.2.0",
    "husky": "^8.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jsdom": "^22.1.0",
    "lint-staged": "^15.2.2",
    "msw": "^1.3.2",
    "msw-auto-mock": "^0.15.0",
    "next-router-mock": "^0.9.13",
    "openapi-typescript": "^6.7.0",
    "prettier": "^3.0.3",
    "utf-8-validate": "^6.0.3",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^0.34.6",
    "vitest-dom": "^0.1.1",
    "typescript-eslint": "8.0.0-alpha.13",
    "eslint": "^9.2.0",
    "eslint-config-next": "^14.2.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "eslint-plugin-testing-library": "^6.2.2",
    "eslint-plugin-unused-imports": "^3.2.0",
    "@eslint/compat": "^1.0.1",
    "@eslint/eslintrc": "^3.0.2",
    "@next/eslint-plugin-next": "^14.2.3"
  }
}
